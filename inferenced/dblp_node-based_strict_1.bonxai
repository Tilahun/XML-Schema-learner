namespace http://example.com/generated

grammar {
	( //school | //publisher | //series ) = {
		attribute href { string }?,
		EMPTY
	}

	//mastersthesis = {
		attribute mdate { string },
		attribute key { string },
		( element author, element title, element year, element school )
	}

	( //article/title | //inproceedings/title ) = {
		( element sub | element sup | element i | element tt )+?
	}

	//article = {
		attribute mdate { string },
		attribute key { string },
		attribute reviewid { string }?,
		attribute rating { string }?,
		( element title | element editor | element journal | element volume | element year | element ee | element cdrom | element author | element month | element url | element publisher | element cite | element pages | element number )+
	}

	( //article/title/sub | //inproceedings/title/sub | //proceedings/title/sub ) = {
		( element sup | element i | element sub )?
	}

	( //article/title/sup | //inproceedings/title/sup | //proceedings/title/sup | //incollection/title/sup ) = {
		( element i | element sub | element sup )?
	}

	//proceedings/title = {
		( element sup* | element sub )?
	}

	( //phdthesis/ee | //cite | //url ) = {
		attribute label { string }?,
		EMPTY
	}

	//proceedings = {
		attribute mdate { string },
		attribute key { string },
		( element title | element volume | element address | element editor | element booktitle | element series | element publisher | element year | element url | element isbn | element author | element number | element cite | element crossref | element month | element note | element ee | element journal )+
	}

	//phdthesis = {
		attribute mdate { string },
		attribute key { string },
		( element author | element title | element year | element school | element series | element number | element publisher | element month | element isbn | element ee )
	}

	//www = {
		attribute mdate { string },
		attribute key { string },
		( ( element author* | element editor* ), element title, ( element booktitle?, element year )?, element url )
	}

	//inproceedings = {
		attribute mdate { string },
		attribute key { string },
		( element title | element author | element pages | element year | element crossref | element booktitle | element ee | element url | element cdrom | element cite | element number | element note | element month )
	}

	( //article/title/i | //inproceedings/title/i ) = {
		( element sub | element sup )+?
	}

	//i/sub = {
		element sub?
	}

	( //book/ee | //chapter ) = {
		EMPTY
	}

	( //article/title/sub/sup | //inproceedings/title/sub/sup | //proceedings/title/sub/sup | //article/title/sup/sup | //inproceedings/title/sup/sup | //proceedings/title/sup/sup | //incollection/title/sup/sup ) = {
		element i?
	}

	//book = {
		attribute mdate { string },
		attribute key { string },
		( element publisher | element series | element cite | element author | element title | element year | element isbn | element editor | element url | element cdrom | element booktitle | element volume | element month | element ee )
	}

	//incollection/title = {
		element sup?
	}

	//incollection = {
		attribute mdate { string },
		attribute key { string },
		( element title | element publisher | element cite | element author | element pages | element year | element booktitle | element url | element ee | element cdrom | element crossref | element isbn | element chapter )+
	}

	//dblp = {
		( element mastersthesis | element article | element proceedings | element phdthesis | element www | element inproceedings | element book | element incollection )+
	}

}
